{
    "id": "haidan",
    "name": "\u6d77\u80c6\u4e4b\u5bb6",
    "domain": "https://www.haidan.video/",
    "encoding": "UTF-8",
    "public": false,
    "parser": "HaiDanSpider",
    "search": {
        "paths": [
            {
                "path": "torrents.php",
                "method": "get"
            }
        ],
        "params": {
            "search": "{keyword}"
        },
        "batch": {
            "delimiter": " ",
            "space_replace": "_"
        }
    },
    "batch": {
        "delimiter": " ",
        "space_replace": "_"
    },
    "browse": {},
    "torrents": {
        "list": {
            "selector": "div.torrent_panel_inner > div.torrent_group"
        },
        "fields": {
            "id": {
                "selector": "a[href*=\"details.php?group_id=\"][href*=\"torrent_id=\"]",
                "attribute": "href",
                "filters": [
                    {
                        "name": "re_search",
                        "args": [
                            "\\d+",
                            1
                        ]
                    }
                ]
            },
            "title_default": {
                "selector": "a[href*=\"details.php?group_id=\"][href*=\"torrent_id=\"]"
            },
            "title_optional": {
                "optional": true,
                "selector": "a[href*=\"details.php?group_id=\"]"
            },
            "title": {
                "text": "{{ fields['title_optional'][0:80] }} {{ fields['title_default'][0:80] }}"
            },
            "category": {
                "selector": "a[href*=\"?cat=\"]",
                "attribute": "href",
                "filters": [
                    {
                        "name": "replace",
                        "args": [
                            "?",
                            ""
                        ]
                    },
                    {
                        "name": "querystring",
                        "args": "cat"
                    }
                ]
            },
            "details": {
                "selector": "a[href*=\"details.php?group_id=\"][href*=\"torrent_id=\"]",
                "attribute": "href"
            },
            "download": {
                "selector": "a[href*=\"download.php?id=\"]",
                "attribute": "href"
            },
            "imdbid": {
                "selector": "div.imdb_100 > a",
                "attribute": "href",
                "filters": [
                    {
                        "name": "re_search",
                        "args": [
                            "tt\\d+",
                            0
                        ]
                    }
                ]
            },
            "date_elapsed": {
                "selector": "div.time_col > span[title]",
                "optional": true
            },
            "date_added": {
                "selector": "div.time_col > span[title]",
                "attribute": "title",
                "optional": true
            },
            "date": {
                "text": "{% if fields['date_elapsed'] or fields['date_added'] %}{{ fields['date_elapsed'] if fields['date_elapsed'] else fields['date_added'] }}{% else %}now{% endif %}",
                "filters": [
                    {
                        "name": "dateparse",
                        "args": "%Y-%m-%d %H:%M:%S"
                    }
                ]
            },
            "size": {
                "selector": "div.video_size"
            },
            "seeders": {
                "selector": "div.seeder_col"
            },
            "leechers": {
                "selector": "div.leecher_col"
            },
            "grabs": {
                "selector": "div.snatched_col"
            },
            "downloadvolumefactor": {
                "case": {
                    "img.pro_free": 0,
                    "img.pro_free2up": 0,
                    "img.pro_50pctdown": 0.5,
                    "img.pro_50pctdown2up": 0.5,
                    "img.pro_30pctdown": 0.3,
                    "*": 1
                }
            },
            "uploadvolumefactor": {
                "case": {
                    "img.pro_50pctdown2up": 2,
                    "img.pro_free2up": 2,
                    "img.pro_2up": 2,
                    "*": 1
                }
            },
            "free_deadline": {
                "default_value": "{% if fields['downloadvolumefactor']==0 %}{{max_time}}{% endif%}",
                "default_value_format": "%Y-%m-%d %H:%M:%S.%f",
                "selector": "img.pro_free,img.pro_free2up",
                "attribute": "onmouseover",
                "filters": [
                    {
                        "name": "re_search",
                        "args": [
                            "\\d+-\\d+-\\d+ \\d+:\\d+:\\d+",
                            0
                        ]
                    },
                    {
                        "name": "dateparse",
                        "args": "%Y-%m-%d %H:%M:%S"
                    }
                ]
            },
            "description": {
                "selector": "div.torrent_group > div.group_content > div.group > div.table_cell > div.name > div.video_name > a",
                "remove": "a,b,img,span",
                "contents": -1
            },
            "labels": {
                "selector": "div.torrent_group > div.group_content > div.group > div.table_cell > div.name > div.video_name_extra > div"
            },
            "minimumratio": {
                "text": 1
            },
            "minimumseedtime": {
                "text": 90000
            }
        }
    },
    "category": {
        "movie": [
            {
