{
    "id": "PTT",
    "name": "PTT",
    "domain": "https://www.pttime.org/",
    "encoding": "UTF-8",
    "public": false,
    "search": {
        "paths": [
            {
                "path": "torrents.php",
                "method": "get"
            }
        ],
        "params": {
            "search": "{keyword}",
            "allsec": true
        },
        "batch": {
            "delimiter": " ",
            "space_replace": "_"
        }
    },
